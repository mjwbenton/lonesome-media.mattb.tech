{
  "name": "mattbenton.co.uk",
  "version": "1.0.0",
  "description": "My personal photography website.",
  "main": "build/index.js",
  "devDependencies": {
    "@types/dotenv": "^4.0.0",
    "@types/react": "^16.0.0",
    "autoprefixer": "^6.3.6",
    "bunyan": "^1.8.1",
    "cssnano": "^3.7.0",
    "dotenv": "^2.0.0",
    "http-server": "^0.9.0",
    "node-sass": "^3.7.0",
    "postcss-cli": "^2.5.2",
    "react": "^15.1.0",
    "staircase-generator": "1.0.0",
    "stylelint": "^6.5.1",
    "stylelint-config-standard": "^8.0.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.3.4"
  },
  "scripts": {
    "all": "npm run build && npm run lint",
    "build": "tsc",
    "lint:css": "stylelint css/*.scss",
    "lint": "npm run lint:css",
    "generate:css": "node-sass css/style.scss | postcss --use autoprefixer --use cssnano -o output/style.css",
    "generate:pages": "ts-node src/index.ts | bunyan -l info",
    "generate": "npm run build && npm run generate:pages && npm run generate:css",
    "serve": "http-server output",
    "upload": "npm run generate && s3_website push"
  },
  "author": "Matt Benton <m@mattb.tech>",
  "license": "UNLICENSED"
}
