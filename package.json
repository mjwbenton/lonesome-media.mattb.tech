{
  "name": "mattbenton.co.uk",
  "version": "1.0.0",
  "description": "My personal photography website.",
  "main": "build/index.js",
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "react": "^15.1.0",
    "staircase": "0.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.3",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "bunyan": "^1.8.1",
    "eslint": "^2.8.0",
    "eslint-plugin-flowtype": "^2.2.7",
    "eslint-plugin-react": "^5.0.1",
    "flow-bin": "^0.23.1",
    "http-server": "^0.9.0",
    "sass": "^0.5.0"
  },
  "scripts": {
    "all": "npm run build && npm run flow && npm run lint",
    "build": "babel src -d build",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint src tst",
    "generate:css": "sass css/style.scss output/style.css",
    "generate:pages": "node build/index.js | bunyan -l info",
    "generate": "npm run build && npm run generate:pages && npm run generate:css",
    "serve": "http-server output"
  },
  "author": "Matt Benton <m@mattb.tech>",
  "license": "UNLICENSED"
}
