{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "strict": true,
    "noImplicitAny": false,
    "lib": ["esnext", "dom"],
    "sourceMap": true,
    "noEmit": true
  },
  "exclude": ["node_modules", "modules"]
}
